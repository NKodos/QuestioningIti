<Window x:Class="Questioning.WindForResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Questioning"
        xmlns:col="clr-namespace:System.Collections;assembly=mscorlib"

        
        mc:Ignorable="d"
        Title="Результат обработки данных" Height="600" Width="700" Loaded="Window_Loaded" Icon="main.ico">

    <Grid>
        <StackPanel>
            <DataGrid x:Name="dataGrid" HorizontalGridLinesBrush="DarkGray" RowBackground="#FF5D5D5D" FontSize="16" IsReadOnly="True"/>
            <Button Name="btnCreatehistogram" Content="Построить гистограмму" HorizontalAlignment="Left" Margin="20 20 0 0" Padding="6, 2" Background="#0067a2" Foreground="White" FontWeight="Light" BorderThickness="0" Click="btnCreatehistogram_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
